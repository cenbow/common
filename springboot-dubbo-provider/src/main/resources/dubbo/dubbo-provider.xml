<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd
    http://code.alibabatech.com/schema/dubbo
    http://code.alibabatech.com/schema/dubbo/dubbo.xsd">


    <dubbo:application name="springboot-provider" />
    <dubbo:registry protocol="zookeeper" address="192.168.99.100:2181" />
    <dubbo:protocol name="dubbo" port="20880" />

    <dubbo:service interface="kelly.springboot.dubbo.api.HelloService"
                   ref="helloServiceImpl" retries="0" timeout="6000" />
</beans>